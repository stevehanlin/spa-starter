{
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "overrides": [
      {
        "files": "*.ts",
        "options": {
          "printWidth": 120
        }
      }
    ],
    "printWidth": 80,
    "quoteProps": "consistent",
    "semi": true,
    "singleAttributePerLine": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
  